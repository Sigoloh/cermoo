{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "eslint class-methods-use-this": 0,
      "eslint padding-line-between-statements": [
        "error",
        { "blankLine": "always", "prev": "*", "next": "*"}
      ]
    }
}
